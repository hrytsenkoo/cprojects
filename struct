#include<stdio.h>
#include<stdlib.h>
#include<string.h>

struct Students {
    char name[50];
    char major[50];
    int age;
    double gpa;
};

int main() {

    struct Students student1;
    student1.age = 20;
    student1.gpa = 3.5;
    strcpy ( student1.name, "Artem");
    strcpy ( student1.major, "Engineer Software");

    struct Students student2;
    student2.age = 30;
    student2.gpa = 4.5;
    strcpy ( student2.name, "Denys");
    strcpy ( student2.major, "Engineer");

    printf("%s", student2.major);

    return 0;
    
}
